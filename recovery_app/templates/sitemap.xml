        <loc>{{ request.build_absolute_uri }}/</loc>
        <lastmod>{% now "Y-m-d" %}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.0</priority>
    </url>
    <url>
        <loc>{{ request.build_absolute_uri }}/services/</loc>
