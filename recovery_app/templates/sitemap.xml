    <url>
        <loc>{{ request.build_absolute_uri }}/reviews/</loc>
        <lastmod>{% now "Y-m-d" %}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.6</priority>
    </url>
    <url>
        <loc>{{ request.build_absolute_uri }}/contact/</loc>
